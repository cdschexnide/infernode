{
  "compilerOptions": {
    "outDir": "../../dist/public/infernode",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "../../../",
    "esModuleInterop": true,
    "types": ["react/next", "react-dom/next"],
    "noEmitOnError": true,
  }
}